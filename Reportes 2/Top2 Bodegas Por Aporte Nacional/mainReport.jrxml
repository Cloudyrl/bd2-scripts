	<parameter name="Pais" class="java.lang.String"/>
	<parameter name="Año" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select h.top2bodegas_p1 as bodega, b.cantidad
from hechos_vinos_catados h, tiempo t, pais p, bodegas_aporte_produccion b 
where (h.top2bodegas_p1 is not null 
or h.top2bodegas_p2 is not null) 
and h.id_tiempo = t.id
and (extract(year from t.año) = $P{Año})
and h.id_pais = p.id
and p.nombre = $P{Pais}
and h.top2bodegas_p1 = b.nombre
and (extract(year from b.fecha) = $P{Año})
union 
select h.top2bodegas_p2 as bodega, b.cantidad
from hechos_vinos_catados h, tiempo t, pais p, bodegas_aporte_produccion b 
where (h.top2bodegas_p1 is not null 
or h.top2bodegas_p2 is not null) 
and h.id_tiempo = t.id
and (extract(year from t.año) = $P{Año})
and h.id_pais = p.id
and p.nombre = $P{Pais}
and h.top2bodegas_p2 = b.nombre
and (extract(year from b.fecha) = $P{Año})]]>
	</queryString>
